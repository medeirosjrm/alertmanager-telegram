#########para executar a imagem Docker##########################
##########apenas uma contrubuição###############################
docker container run -d -p 8080:8080 --name alertmanager-telegram --mount type=bind,src=/opt/telegram/templates/default.html,dst=/telegram -e TELEGRAM_CHAT_ID="-chateID" -e TELEGRAM_TOKEN="tocken" janwh/alertmanager-telegram
